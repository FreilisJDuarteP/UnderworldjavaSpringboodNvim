--project wizard
vim.api.nvim_create_user_command("JavaNewProject", function()
	vim.cmd("normal <leader>jp")
end, {})
local Terminal = require("toggleterm.terminal").Terminal

local function run(cmd)
	Terminal:new({
		cmd = cmd,
		direction = "float",
		close_on_exit = false,
	}):toggle()
end

vim.keymap.set("n", "<leader>jp", function()
	vim.ui.select({
		"Gradle",
		"Maven",
		"Spring Boot",
	}, { prompt = "Create Java Project" }, function(choice)
		if not choice then return end

		if choice == "Gradle" then
			run("bash -c 'read -p \"Project name: \" n; mkdir $n && cd $n && gradle init'")
		elseif choice == "Maven" then
			run(
				"bash -c 'read -p \"Project name: \" n; read -p \"GroupId: \" g; mkdir $n && cd $n && mvn archetype:generate -DgroupId=${g:-com.example} -DartifactId=$n -DinteractiveMode=false'")
		elseif choice == "Spring Boot" then
			run("bash -c 'read -p \"Name: \" n; spring init $n'")
		end
	end)
end, { desc = "Java: New Project Wizard" })
