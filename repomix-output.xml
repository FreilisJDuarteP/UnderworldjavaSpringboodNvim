return {
  {
    "nvim-treesitter/nvim-treesitter",
    lazy = false,
    build = ":TSUpdate",

    config = function()
      local ts = require("nvim-treesitter")

      -- 1. Configuraci√≥n b√°sica (setup)
      ts.setup({
        -- Opcional: solo si quieres mover donde se instalan
        install_dir = vim.fn.stdpath("data") .. "/site",
      })

      -- 2. Instalaci√≥n de parsers (el reemplazo de ensure_installed)
      -- Esto es un "no-op" si ya est√°n instalados, as√≠ que es seguro tenerlo aqu√≠
      ts.install({ "lua", "java", "markdown", "markdown_inline", "bash" })
    end,
  },
}
